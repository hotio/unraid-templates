<?xml version="1.0"?>
<Container version="2">
  <Name>qui</Name>
  <Repository>ghcr.io/hotio/qui:latest</Repository>
  <Registry>https://github.com/hotio/qui/pkgs/container/qui</Registry>
  <Branch>
    <Tag>release</Tag>
