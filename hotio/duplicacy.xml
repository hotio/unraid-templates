<?xml version="1.0"?>
<Container version="2">
  <Name>duplicacy</Name>
  <Repository>ghcr.io/hotio/duplicacy</Repository>
  <Registry>https://github.com/hotio/duplicacy/pkgs/container/duplicacy</Registry>
  <Branch>
    <Tag>release</Tag>
    <TagDescription>Stable channel</TagDescription>
  </Branch>
  <Branch>
    <Tag>testing</Tag>
    <TagDescription>Latest channel</TagDescription>
  </Branch>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://hotio.dev/discord</Support>
  <Project>https://hotio.dev/containers/duplicacy</Project>
  <Category>Backup:</Category>
  <TemplateURL>false</TemplateURL>
  <Icon>https://hotio.dev/webhook-avatars/duplicacy.png</Icon>
  <Overview>A new generation cross-platform cloud backup tool.</Overview>
  <Description/>
  <MyIP/>
  <WebUI>http://[IP]:[PORT:3875]</WebUI>
  <ExtraParams>--hostname=duplicacy</ExtraParams>
  <PostArgs/>
  <Network>bridge</Network>
  <Config Name="Host Port for 3875" Target="3875" Default="3875" Mode="tcp" Description="Container Port: 3875" Type="Port" Display="always-hide" Required="true" Mask="false"/>
  <Config Name="Host Path for /config" Target="/config" Default="" Mode="rw" Description="Container Path: /config" Type="Path" Display="always-hide" Required="true" Mask="false"/>
  <Config Name="Host Path for /cache" Target="/cache" Default="" Mode="rw" Description="Container Path: /cache" Type="Path" Display="always-hide" Required="true" Mask="false"/>
  <Config Name="Host Path for /logs" Target="/logs" Default="" Mode="rw" Description="Container Path: /logs" Type="Path" Display="always-hide" Required="true" Mask="false"/>
  <Config Name="Host Path for /data" Target="/data" Default="" Mode="rw" Description="Container Path: /data" Type="Path" Display="always-hide" Required="true" Mask="false"/>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
  <Config Name="UMASK" Target="UMASK" Default="002" Mode="" Description="Container Variable: UMASK" Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
</Container>
